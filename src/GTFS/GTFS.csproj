<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>GTFS-Custom</AssemblyName>
    <PackageId>GTFS-Custom</PackageId>
    <Title>GTFS-Custom</Title>
    <Authors>Itinero and contributors</Authors>
    <Description>GTFS read/write library.</Description>
    <PackageProjectUrl>https://github.com/FleQ-inc/GTFS</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.github.com/OsmSharp/GTFS/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>https://raw.github.com/OsmSharp/GTFS/master/icon.bmp</PackageIconUrl>
    <PackageTags>routing,GTFS,transit</PackageTags>
    <PackageVersion>3.0.0-pre005</PackageVersion>
    <RepositoryUrl>https://github.com/FleQ-inc/GTFS</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

</Project>
